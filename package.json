{
  "name": "medistraction",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:vercel": "ADAPTER=vercel vite build",
    "build:mobile": "ADAPTER=static vite build && npx cap sync",
    "build:apk": "npm run build:mobile && cd android && ./gradlew assembleDebug",
    "build:apk-release": "npm run build:mobile && cd android && ./gradlew assembleRelease",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@vite-pwa/sveltekit": "^1.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "workbox-window": "^7.4.0"
  },
  "type": "module",
  "dependencies": {
    "@capacitor-community/native-audio": "^8.0.0",
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "@sveltejs/adapter-vercel": "^6.3.1",
    "pocketbase": "^0.26.8"
  }
}
